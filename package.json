{
  "name": "react-redux-starter-kit",
  "version": "2.0.0-alpha.1",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run dev",
    "dev": "DEBUG=app:* NODE_ENV=development babel-node bin/devServer",
    "build:prod": "npm run clean & NODE_ENV=production webpack --config bin/webpack.prod.config.js --no-colors",
    "build:dev": "NODE_ENV=development webpack --config bin/webpack.dev.config.js --display-error-details",
    "test": "NODE_ENV=test DEBUG=app:* babel-node ./node_modules/karma/bin/karma start bin/karma.ci.conf",
    "test:dev": "NODE_ENV=test-dev DEBUG=app:* babel-node ./node_modules/karma/bin/karma start bin/karma.conf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "history": "^2.0.0",
    "koa": "^2.0.0-alpha.3",
    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-proxy": "^0.5.0",
    "koa-static": "^2.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.0.0",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.2",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-enzyme": "^0.4.0",
    "cheerio": "^0.20.0",
    "codecov": "^1.0.1",
    "css-loader": "^0.23.0",
    "cssnano": "^3.3.2",
    "enzyme": "^2.0.0",
    "eslint": "^2.1.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^2.2.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-flow-vars": "^0.2.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^4.0.0",
    "eslint-plugin-standard": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.7.1",
    "imports-loader": "^0.6.5",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
